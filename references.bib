	pages = {257--279},
	file = {ScienceDirect Full Text PDF:/home/phil/.mozilla/firefox/074pwc7t.default/zotero/storage/2K2GECC3/Akyildiz et al. - 2005 - Underwater acoustic sensor networks research chal.pdf:application/pdf;ScienceDirect Snapshot:/home/phil/.mozilla/firefox/074pwc7t.default/zotero/storage/CP5BM863/S1570870505000168.html:text/html}
