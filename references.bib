	pages = {000299--000304},
	file = {IEEE Xplore Abstract Record:/home/phil/.mozilla/firefox/074pwc7t.default/zotero/storage/9FJSMTED/6754963.html:text/html;IEEE Xplore Full Text PDF:/home/phil/.mozilla/firefox/074pwc7t.default/zotero/storage/R6SX8ZG8/Souza et al. - 2013 - End-to-end authentication in Under-Water Sensor Ne.pdf:application/pdf}
