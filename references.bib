	month = oct,
	year = {2008},
	keywords = {Authentication, channel characteristics, Character generation, communication security, Cryptography, Data mining, data privacy, Data security, encryption, key generation system, oceanographic techniques, Oceans, Robustness, secure symmetric key, spatio-temporal variable ocean environment, telecommunication channels, telecommunication security, underwater acoustic communication, Underwater acoustics, Underwater communication},
